{
  "name": "chat-frontier-flora",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "cd apps/web && npx expo export:web",
    "dev:web": "cd apps/web && npm run web",
    "dev:mobile": "npm run start -w @chat-frontier-flora/mobile",
    "build:web": "cd apps/web && npx expo export:web",
    "build:mobile": "cd apps/mobile && eas build",
    "test": "npm run test --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present",
    "postinstall": "npm run build -w @chat-frontier-flora/shared"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "stream-browserify": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@react-navigation/native": "^7.1.10",
    "@react-navigation/stack": "^7.1.1",
    "react-native-screens": "^4.11.1",
    "react-native-safe-area-context": "^5.4.1"
  },
  "overrides": {
    "react": "^18.2.0 <19.0.0",
    "react-dom": "^18.2.0 <19.0.0",
    "react-test-renderer": "^18.2.0 <19.0.0",
    "@types/react": "~18.2.45"
  }
}
