{
  "name": "chat-frontier-flora",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "cd apps/web && rm -rf .expo/ dist/ .cache/ 2>/dev/null || true && npx expo export --platform web && mkdir -p ../../dist && cp -r dist/* ../../dist/",
    "build:web": "cd apps/web && rm -rf .expo/ dist/ .cache/ 2>/dev/null || true && npx expo export --platform web"
  },
  "dependencies": {
    "expo": "~50.0.0",
    "nativewind": "^4.1.23",
    "react-router-dom": "^6.30.1",
    "resolve-from": "^4.0.0",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "stream-browserify": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0",
    "@types/react": "~18.2.45"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0",
    "@types/react": "~18.2.45"
  },
  "optionalDependencies": {
    "lightningcss-linux-x64-gnu": "1.30.1",
    "lightningcss-darwin-arm64": "1.30.1"
  }
}
